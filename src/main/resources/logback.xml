<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="false">

    <logger name="br.com.hossomi.kss.kafka" level="info"/>

    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} | %-5level | %-50(%logger{50}) - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="info">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>